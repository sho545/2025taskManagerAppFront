{
  "name": "task-app-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-api": "dotenv -- rimraf --glob \"./src/apis/*.ts\" api-tmp && dotenv -- openapi-generator-cli generate -i %BACKEND_API_SPEC_PATH% -g typescript-axios -o ./api-tmp && copyfiles -u 1 \"api-tmp/*.ts\" src/apis && rimraf api-tmp",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest"
  },
  "dependencies": {
    "@date-io/date-fns": "^3.2.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-date-pickers": "^8.10.0",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-router-dom": "^7.6.3",
    "storybook": "^9.1.2",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@date-fns/utc": "^2.1.1",
    "@eslint/js": "^9.30.1",
    "@openapitools/openapi-generator-cli": "^2.22.0",
    "@storybook/addon-vitest": "^9.1.2",
    "@storybook/react-vite": "^9.1.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.1",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
